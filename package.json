{
  "name": "sails-postgresql",
  "description": "a postgreSQL adapter for Waterline and Sails.js",
  "version": "0.10.16",
  "author": "Cody Stoltman <particlebanana@gmail.com>",
  "url": "http://github.com/balderdashy/sails-postgresql",
  "keywords": [
    "postgresql",
    "orm",
    "waterline",
    "sails"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-postgresql.git"
  },
  "dependencies": {
    "async": "~1.3.0",
    "camelize": "^1.0.0",
    "fnv-plus": "^1.2.12",
    "knex": "^0.8.6",
    "lodash": "^3.10.0",
    "pg": "~4.4.0",
    "pg-array": "^0.5.0",
    "waterline-cursor": "0.0.5",
    "waterline-errors": "~0.10.0",
    "waterline-sequel": "^0.5.0"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "captains-log": "~0.11.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "mocha": "*",
    "should": "*",
    "waterline-adapter-tests": "~0.10.7"
  },
  "scripts": {
    "test": "make test"
  },
  "main": "dist/adapter",
  "directories": {
    "lib": "./lib"
  },
  "license": "MIT",
  "bugs": "https://github.com/balderdashy/sails-postgresql/issues",
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.0",
    "interfaces": [
      "semantic",
      "queryable",
      "associations",
      "sql",
      "migratable"
    ],
    "features": [
      "unique",
      "autoIncrement",
      "crossAdapter"
    ]
  }
}
